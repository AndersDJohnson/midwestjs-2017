  "scripts": {
    "author-watch": "./util/gen-ex.bash && run-p watch master-watch",
    "clean": "mkdirp dist && rimraf dist/*",
    "build": "cross-env BABEL_ENV=commonjs WEBPACK_MODE=single webpack",
    "gen-html": "node util/build-html.js",
    "lint": "eslint --ext .js --ext .jsx subjects",
    "master-watch": "chokidar 'subjects/**/master/**/*.js*' -c ./util/gen-ex.bash",
    "preauthor-watch": "./util/confirm-del.bash",
    "prebuild": "npm run clean",
    "postbuild": "echo \"Finished\"",
    "prepublish": "npm run prod-build",
    "prewatch": "npm run clean",
    "prod-build": "npm run build --production",
    "start": "npm run watch",
    "watch": "run-p watch:**",
    "watch:wsServer": "node ./util/ws-echo-server.js",
    "watch:webpack": "cross-env BABEL_ENV=commonjs webpack --watch"
  },
